{
    "name": "achrafbardan/kvk-sdk",
    "description": "KVK SDK for php 8.1 and up.",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-http/curl-client": "^2.2",
        "laminas/laminas-diactoros": "^2.8",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Achrafbardan\\KvkSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Achraf Bardan",
            "email": "achrafbardan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0",
        "php-http/client-common": "^2.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
